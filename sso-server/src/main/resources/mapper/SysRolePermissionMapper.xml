<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhsl.mapper.SysRolePermissionMapper">
  <resultMap id="BaseResultMap" type="com.zhsl.model.SysRolePermission">
    <result column="permission_id" jdbcType="VARCHAR" property="permissionId" />
    <result column="role_id" jdbcType="VARCHAR" property="roleId" />
  </resultMap>
  <insert id="insert" parameterType="com.zhsl.model.SysRolePermission">
    insert into sys_role_permission (permission_id, role_id)
    values (#{permissionId,jdbcType=VARCHAR}, #{roleId,jdbcType=VARCHAR})
  </insert>
  <select id="selectAll" resultMap="BaseResultMap">
    select permission_id, role_id
    from sys_role_permission
  </select>
</mapper>